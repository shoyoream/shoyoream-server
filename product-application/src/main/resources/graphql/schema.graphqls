type DefaultResponse {
    id: String
}

enum GoodsType {
    GI
    NO_GI_TOP
    NO_GI_SHORTS
    BELTS
    ACCESSORIES
}

enum GoodsSize {
    A0
    A1
    A2
    A3
    S
    M
    L
    XL
    XXL
}

input GoodsInput {
    brandId: String!
    goodsType: GoodsType!
    goodsName: String!
    goodsCode: String!
    goodsSize: GoodsSize!
}
